{
  "dataRoles": [
    {
      "displayName": "Rows",
      "name": "rows",
      "kind": "Grouping"
    },
    {
      "displayName": "Columns",
      "name": "columns",
      "kind": "Grouping"
    },
    {
      "displayName": "Values",
      "name": "measures",
      "kind": "Measure"
    }
  ],
  "dataViewMappings": [
    {
      "categorical": {
        "categories": {
          "for": {
            "in": "rows"
          }
        },
        "values": {
          "group": {
            "by": "columns", 
            "select":[
              { "bind": { "to": "measures" } }
            ]
          }
        }
      }
    }
  ]
}